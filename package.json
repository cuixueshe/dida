{
  "name": "vue3-dida",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.17.0",
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "bootstrap": "cross-env CYPRESS_INSTALL_BINARY=0 pnpm i",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "pnpm test-unit-run",
    "test-unit": "vitest",
    "test-component": "cypress open --component",
    "test-e2e": "cross-env NODE_ENV=developer cypress open --e2e",
    "test-unit-run": "vitest --run",
    "test-component-run": "cypress run --component",
    "test-e2e-run": "cross-env NODE_ENV=developer cypress run --e2e",
    "prepare": "simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dep:up": "taze -I major"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.1.10",
    "ink-mde": "^0.18.1",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "4.1.6",
    "vue3-emoji-picker": "^1.1.7",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@iconify-json/carbon": "^1.1.13",
    "@iconify-json/mdi": "^1.1.41",
    "@iconify/vue": "^4.0.2",
    "@peculiar/webcrypto": "^1.4.1",
    "@types/node": "^18.11.18",
    "@unocss/reset": "^0.48.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue-macros/volar": "^0.5.10",
    "@vue/test-utils": "^2.2.7",
    "@vueuse/core": "^9.10.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.3.0",
    "cypress-promise": "^1.1.0",
    "eslint": "^8.31.0",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.20",
    "postcss-nested": "^6.0.0",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.8.5",
    "typescript": "^4.9.4",
    "unocss": "^0.48.3",
    "unplugin-vue-macros": "^1.0.3",
    "vite": "^4.0.4",
    "vitest": "^0.27.0",
    "vue-tsc": "^1.0.24"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint --edit \"${1}\""
  },
  "lint-staged": {
    "*.{js,ts,json,md,jsx,tsx,vue,yml,yaml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu",
    "ignorePatterns": [
      "**/*.cy.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
